{
  "name": "BlocksTest",
  "author": "Jens Klimke",
  "model": {
    "targetVelocity": {
      "out": "target",
      "type": "input"
    },
    "acceleration": {
      "in": "control",
      "state": {
        "value": "init.acceleration"
      },
      "type": "output"
    },
    "velocity": {
      "in": "system",
      "state": {
        "value": "init.velocity"
      },
      "type": "output"
    },
    "error": {
      "in": [
        {
          "signal": "target",
          "sign": "+"
        },
        {
          "signal": "previous",
          "sign": "-"
        }
      ],
      "out": "error",
      "type": "sum"
    },
    "pid": {
      "name": "PID",
      "in": "error",
      "out": "control",
      "state": {
        "sum": "init.pid.sum",
        "err": "init.pid.error"
      },
      "parameters": {
        "kP": {},
        "kI": {},
        "kD": {}
      }
    },
    "integral": {
      "symbol": "$\\int$",
      "in": "control",
      "out": "system",
      "value": "init.integral"
    },
    "delay": {
      "symbol": "$z^{-1}$",
      "in": "system",
      "out": "previous",
      "value": "init.delay",
      "type": "delay"
    }
  }
}